{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "plugins": [
      {
        "name": "next"
      }
    ],
"paths": {
  "@/":["./"],
  "@/*": ["./src/*"],
  "@/components/*": ["components/*"],
  "@/constants/*": ["constants/*"],
  "@/small_components/*": ["small_components/*"],
  "@/hooks/*": ["hooks/*"],
  "@/library/*": ["library/*"],
  "@/models/*": ["models/*"]  //Corrected syntax here
}
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
}
